{
  "name": "gatsby-starter-netlify-cms",
  "description": "Example Gatsby, and Netlify CMS project",
  "version": "1.1.1",
  "author": "Austin Green",
  "dependencies": {
    "gatsby": "^1.9.213",
    "gatsby-link": "^1.6.37",
    "gatsby-plugin-feed": "^1.3.20",
    "gatsby-plugin-google-analytics": "^1.0.29",
    "gatsby-plugin-manifest": "^1.0.22",
    "gatsby-plugin-offline": "^1.0.16",
    "gatsby-plugin-react-helmet": "^2.0.5",
    "gatsby-remark-images": "^1.5.50",
    "gatsby-remark-prismjs": "^2.0.2",
    "gatsby-source-filesystem": "^1.5.23",
    "gatsby-transformer-remark": "^1.7.40",
    "prismjs": "^1.24.0",
    "prop-types": "^15.6.0",
    "purgecss": "^0.21.0",
    "react": "^16.2.0",
    "react-helmet": "^5.2.0",
    "sharp": "^0.20.5",
    "tailwindcss": "^0.5.2",
    "typeface-josefin-sans": "0.0.54"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "start": "npm run develop",
    "build": "npm run css && gatsby build",
    "develop": "npm run tailwind && gatsby develop",
    "tailwind": "tailwind build ./src/layouts/index.css -c ./tailwind.js -o ./src/layouts/style.css",
    "purgecss": "purgecss -c purgecss.config.js -o ./src/layouts",
    "css": "npm run tailwind && npm run purgecss",
    "serve": "gatsby serve",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "prettier": "^1.7.4"
  }
}
